			SA.MouseButton1Click:Connect(function()
				SA.Text = "Silent Aim Enabled"
				local Camera = game:GetService("Workspace").CurrentCamera
				local Players = game:GetService("Players")
				local LocalPlayer = game:GetService("Players").LocalPlayer

				local function GetClosestPlayer()
					local ClosestPlayer = nil
					local FarthestDistance = math.huge

					for i, v in pairs(Players.GetPlayers(Players)) do
						if v ~= LocalPlayer and v.Character and v.Character.FindFirstChild(v.Character, "HumanoidRootPart") then
							local DistanceFromPlayer = (LocalPlayer.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude

							if DistanceFromPlayer < FarthestDistance then
								FarthestDistance = DistanceFromPlayer
								ClosestPlayer = v
							end
						end
					end

					if ClosestPlayer then
						return ClosestPlayer
					end
				end

				local GameMetaTable = getrawmetatable(game)
				local OldGameMetaTableNamecall = GameMetaTable.__namecall
				setreadonly(GameMetaTable, false)

				GameMetaTable.__namecall = newcclosure(function(object, ...)
					local NamecallMethod = getnamecallmethod()
					local Arguments = {...}

					if tostring(NamecallMethod) == "FindPartOnRayWithIgnoreList" then
						local ClosestPlayer = GetClosestPlayer()

						if ClosestPlayer and ClosestPlayer.Character then
							Arguments[1] = Ray.new(Camera.CFrame.Position, (ClosestPlayer.Character.Head.Position - Camera.CFrame.Position).Unit * (Camera.CFrame.Position - ClosestPlayer.Character.Head.Position).Magnitude)
						end
					end

					return OldGameMetaTableNamecall(object, unpack(Arguments))
				end)

				setreadonly(GameMetaTable, true)
			end)

			Rejoin.MouseButton1Click:Connect(function()
				local tpservice= game:GetService("TeleportService")
				local plr = game.Players.LocalPlayer

				tpservice:Teleport(game.PlaceId, plr)
			end)

			NN.MouseButton1Click:Connect(function()
				game.Players.LocalPlayer.Character.Head.NameTag:Destroy()
				NN.Text = "Nametag Hidden!"
				wait(2.5)
				NN.Text = "Hide Nametag"
			end)


			local AutoHeal = false

			AH.MouseButton1Click:Connect(function()
				if AutoHeal then
					AutoHeal = false
					AH.Text = 'Auto Heal Disabled'
				else
					AutoHeal = true
					AH.Text = 'Auto Heal Enabled'
				end			
			end)

			RunService.RenderStepped:Connect(function()
				local Humanoid = Player.Character:FindFirstChild('Humanoid')
				if Humanoid then
					local X, Z = 0, 0

					if UserInputService:IsKeyDown(Enum.KeyCode.W) then
						Z += -1
					end

					if UserInputService:IsKeyDown(Enum.KeyCode.A) then
						X += -1
					end

					if UserInputService:IsKeyDown(Enum.KeyCode.S) then
						Z += 1
					end

					if UserInputService:IsKeyDown(Enum.KeyCode.D) then
						X += 1
					end

					Humanoid:Move(Vector3.new(X, 0, Z), true)
				end

				if AutoHeal then
					local Character = workspace:FindFirstChild(Player.Name)

					if Character then
						if Character.Humanoid.Health <= 35 and Character.Humanoid.Health > 0 then
							game:GetService("Players").LocalPlayer.Backpack["Health Potion"].DrinkPotion:InvokeServer()
						end
					end
				end
			end)


			LW.MouseButton1Click:Connect(function()
				while true do
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30
					wait()
				end
			end)

			while wait (0.1) do
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(27, 20, 255)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(170, 0, 127)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(85, 0, 127)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(255, 0, 255)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(170, 85, 255)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(255, 170, 255)
				wait(0.5)
				TextLabel.TextColor3 = Color3.fromRGB(170, 85, 127)
			end
	end
end)

-- // Chat Bypass

Player.Chatted:Connect(function(Message)
	if Message:sub(1,105):lower() == ".chatbypass" then
		AnimalESPLoaded.Visible = false
		AntiAFKLoaded.Visible = false
		AntiCharDisablerLoaded.Visible = false
		AntiLassoLoaded.Visible = false
		CMDXLoaded.Visible = false
		ChatBypassLoaded.Visible = true
		BlitzkriegBrigadeUILoaded.Visible = false
		CharacterRefreshed.Visible = false
		Instructions.Visible = false
		DisplayNamesRemoved.Visible = false
		Loaded.Visible = false
		MoneyScriptActivated.Visible = false
		MoneyScriptLaunched.Visible = false
		wait(2)
		ChatBypassLoaded.Visible = false
		getgenv().method = "fn" -- put the two letters (method) here. available ones are pq, bj, dq and hu. fn is recommended by default.
		loadstring(game:HttpGet("https://raw.githubusercontent.com/daddysyn/synergy/additional/betterbypasser",true))()
	end
end)

