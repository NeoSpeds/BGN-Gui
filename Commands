-- // Anti Char Disabler

Player.Chatted:Connect(function(Message)
	if Message:sub(1,17):lower() == ".antichardisabler" then
		AnimalESPLoaded.Visible = false
		AntiAFKLoaded.Visible = false
		AntiCharDisablerLoaded.Visible = true
		AntiLassoLoaded.Visible = false
		CMDXLoaded.Visible = false
		ChatBypassLoaded.Visible = false
		BlitzkriegBrigadeUILoaded.Visible = false
		CharacterRefreshed.Visible = false
		Instructions.Visible = false
		DisplayNamesRemoved.Visible = false
		Loaded.Visible = false
		MoneyScriptActivated.Visible = false
		MoneyScriptLaunched.Visible = false
		wait(2)
		AntiCharDisablerLoaded.Visible = false
		while wait () do
			game:GetService("Players").LocalPlayer.States.BackpackDisabled.Value = false
			game:GetService("Players").LocalPlayer.States.Robbable.Value = false
			game:GetService("Players").LocalPlayer.StateConfig.CharacterDisabled.Value = false
end
	end
end)

-- // Auto Farm Launcher

Player.Chatted:Connect(function(Message)
	if Message:sub(1,20):lower() == ".launchautofarm" then
		AnimalESPLoaded.Visible = false
		AntiAFKLoaded.Visible = false
		AntiCharDisablerLoaded.Visible = false
		AntiLassoLoaded.Visible = false
		CMDXLoaded.Visible = false
		ChatBypassLoaded.Visible = false
		BlitzkriegBrigadeUILoaded.Visible = false
		CharacterRefreshed.Visible = false
		Instructions.Visible = false
		DisplayNamesRemoved.Visible = false
		Loaded.Visible = false
		MoneyScriptActivated.Visible = false
		MoneyScriptLaunched.Visible = true
		wait(2)
		MoneyScriptLaunched.Visible = false
coroutine.wrap(function()
    if not getgenv then error("You need getgenv in order to execute this!",2) return end
    local a = "Made by h17s3#2734"
    while task.wait() and #a>16 and a  do
        if #a-3 <100 then
            pcall(function()
                if getgenv then
                    getgenv().hookfunction = NIL 
                end
            end)
            pcall(function()
                if getgenv then
                    getgenv().hookfunc = NIL 
                end
            end)
        end
    end
end)()

wait (2)

